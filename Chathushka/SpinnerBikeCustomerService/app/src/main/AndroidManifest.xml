        android:supportsRtl="true"
        android:theme="@style/Theme.SpinnerBikeCustomerService">
        <activity android:name=".NinthActivity"></activity>
        <activity android:name=".EightActivity"></activity>
        <activity android:name=".SeventhActivity" />
